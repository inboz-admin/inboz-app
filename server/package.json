{
  "name": "INBOZ",
  "version": "1.0.0",
  "description": "INBOZ",
  "author": "INBOZ",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "start:worker": "nest start --watch --entryFile worker",
    "start:worker:debug": "nest start --debug --watch --entryFile worker",
    "start:worker:prod": "node dist/src/worker.js",
    "start:scheduler": "nest start --watch --entryFile scheduler",
    "start:scheduler:debug": "nest start --debug --watch --entryFile scheduler",
    "start:scheduler:prod": "node dist/src/scheduler.js",
    "build:prod": "npm run build && npm prune --production",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --maxWorkers=2 --detectOpenHandles --forceExit --verbose",
    "test:watch": "jest --watch --maxWorkers=2 --verbose",
    "test:cov": "jest --coverage --maxWorkers=2 --detectOpenHandles --forceExit --verbose",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --maxWorkers=2 --detectOpenHandles --forceExit --verbose",
    "test:unit:module": "jest --maxWorkers=2 --detectOpenHandles --forceExit --verbose test/unit",
    "test:e2e:module": "jest --config ./test/jest-e2e.json --maxWorkers=2 --detectOpenHandles --forceExit --verbose test/e2e",
    "test:all": "npm run test:unit && npm run test:e2e",
    "generate:vapid-keys": "node scripts/generate-vapid-keys.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "db:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "db:test:migrate": "node scripts/test-db.js migrate",
    "db:test:migrate:undo:all": "node scripts/test-db.js migrate:undo:all",
    "db:test:seed": "node scripts/test-db.js seed",
    "db:test:seed:undo:all": "node scripts/test-db.js seed:undo:all",
    "db:test:reset": "npm run db:test:migrate:undo:all && npm run db:test:migrate && npm run db:test:seed",
    "db:test:setup": "npm run db:test:migrate && npm run db:test:seed",
    "test:load": "k6 run performance-tests/api-load-test.js"
  },
  "dependencies": {
    "@bull-board/api": "^6.13.0",
    "@bull-board/express": "^6.13.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/platform-socket.io": "^11.1.6",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/sequelize": "^11.0.0",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.6",
    "@redis/client": "^5.8.2",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.1",
    "axios": "^1.12.2",
    "bullmq": "^5.61.0",
    "cache-manager": "^7.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "exceljs": "^4.4.0",
    "express-basic-auth": "^1.2.1",
    "fast-csv": "^5.0.5",
    "googleapis": "^160.0.0",
    "ioredis": "^5.7.0",
    "moment-timezone": "^0.6.0",
    "multer": "^2.0.2",
    "mysql2": "^3.15.0",
    "nodemailer": "^7.0.6",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.17.2",
    "razorpay": "^2.9.6",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "socket.io": "^4.8.1",
    "stripe": "^20.1.0",
    "uuid": "^13.0.0",
    "web-push": "^3.6.7",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/register": "^7.28.3",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/moment-timezone": "^0.5.13",
    "@types/node": "^24.5.2",
    "@types/pdfkit": "^0.17.3",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.1.3",
    "prettier": "^3.6.2",
    "sequelize-cli": "^6.6.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.3",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": "test/unit/.*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "src/resources/organizations/**/*.ts",
      "!src/**/*.spec.ts",
      "!src/**/*.interface.ts",
      "!src/**/*.dto.ts",
      "!src/**/*.entity.ts",
      "!src/**/index.ts",
      "!src/**/*.config.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "text-summary",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "testEnvironment": "node",
    "testTimeout": 10000,
    "bail": false,
    "verbose": true,
    "detectOpenHandles": true,
    "forceExit": true
  }
}
